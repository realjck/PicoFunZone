--road

function road_move()
	rx-=1
	if (rx<=-32) rx=0
end

function road_draw()
	local x,y
	--scenery
	local l=(p.level-1)%3
	local s
	if (l==0) s=8
	if (l==1) s=12
	if (l==2) s=14
	for x=rx,128,16 do
		spr(s,x,0,2,2)
	end
	--road
	for y=40,100,30 do
		for x=rx,128,32 do				
			rectfill(x,y,x+12,y+2,7)
		end
	end
	--sun
		spr(10,80,0,2,1)
end
