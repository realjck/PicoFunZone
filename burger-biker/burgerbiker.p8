--collisions

function check_collisions()
	for i=1,#objs do
		if objs[i].x < p.x+28
		and objs[i].x > p.x-8
		and objs[i].y < p.y+32
		and objs[i].y > p.y+20
		and objs[i].type==1
		then
			--gems
			p.score+=1
			sfx(0)
			for j=0,10 do
				add_new_dust(objs[i].x+4,objs[i].y+4,rnd(4)-2,rnd(4)-2,25,rnd(3)+1,0.05,
				{6,9,9,9,10,10,10,10,10,7,7,7})
			end
			del(objs,objs[i])

			return
		elseif (objs[i].x < p.x+28
		and objs[i].x > p.x
		and objs[i].y < p.y+28
		and objs[i].y > p.y+22
		and objs[i].type==0)
		or  (objs[i].x < p.x+28
		and objs[i].x > p.x
		and objs[i].y < p.y+28
		and objs[i].y > p.y+18
		and objs[i].type==2)
		then		
			--holes & bullets
			music(-1)
			sfx(1)
			spr(41,p.x+22,p.y-4,2,1)
			p.life-=1
			p.losing=120			
		end
	end
end
